{
  "peerDependencies": {
    "@storybook/react": ">=5.3.9",
    "antd": ">=4.4.3",
    "imask": "6.0.5",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "dependencies": {
    "@ant-design/icons": "4.2.1",
    "@reach/router": "1.3.4",
    "@rjsf/antd": "2.2.0",
    "@rjsf/core": "2.2.0",
    "@sentry/react": "^5.20.1",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "antd": "latest",
    "antd-img-crop": "^3.9.0",
    "dayjs": "1.8.28",
    "firebase": "7.15.5",
    "imask": "6.0.5",
    "moment": "latest",
    "query-string": "^6.13.1",
    "react": "latest",
    "react-color": "^2.18.1",
    "react-currency-masked-input": "^2.5.0",
    "react-device-detect": "^1.13.1",
    "react-dom": "latest",
    "react-image": "^4.0.3",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-intl": "^5.4.5",
    "react-markdown": "4.3.1",
    "react-social-login-buttons": "3.1.0",
    "slugify": "^1.4.4",
    "tachyons": "^4.12.0",
    "typescript": "^3.9.6",
    "use-query-params": "^1.1.6",
    "uuid": "^8.2.0"
  },
  "keywords": [],
  "name": "altzap",
  "description": "Compartilhe seu cardÃ¡pio e receba pedidos pelo Whatsapp",
  "version": "0.0.180",
  "devDependencies": {
    "@bit/bit.envs.compilers.typescript": "^3.1.49",
    "@sambego/storybook-state": "^2.0.1",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/reach__router": "^1.3.5",
    "@types/react-input-mask": "^2.0.5",
    "@types/storybook__react": "^5.2.1",
    "@types/uuid": "^8.0.0",
    "@vtex/prettier-config": "^0.3.1",
    "eslint-config-vtex-react": "^6.7.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.1",
    "source-map-explorer": "^2.4.2"
  },
  "optionalDependencies": {
    "cypress": "^4.9.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "emulators": "firebase emulators:start --import=dev",
    "firestore-export": "firebase emulators:export dev",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,js,json}\"",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "bit-export": "bit export lucis.alt",
    "bit-tag-minor": "bit tag --all --minor",
    "bit-tag-patch": "bit tag --all --patch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "bit": {
    "env": {
      "compiler": {
        "bit.envs/compilers/react-typescript@3.1.56": {
          "rawConfig": {
            "compilerPath": "typescript/bin/tsc",
            "compilerArguments": [
              "--declaration"
            ],
            "compiledFileTypes": [
              ".ts",
              ".tsx"
            ],
            "configFileName": "tsconfig.json",
            "tsconfig": {
              "compilerOptions": {
                "outDir": "dist",
                "target": "ES2015",
                "sourceMap": false,
                "moduleResolution": "Node",
                "esModuleInterop": true,
                "module": "ES6",
                "allowSyntheticDefaultImports": true,
                "resolveJsonModule": true,
                "declaration": true,
                "experimentalDecorators": true,
                "inlineSourceMap": false,
                "inlineSources": false,
                "lib": [
                  "dom",
                  "es2015",
                  "es7"
                ],
                "jsx": "react",
                "rootDir": "./",
                "removeComments": true,
                "typeRoots": [
                  "./node_modules/@types"
                ],
                "importHelpers": false
              },
              "include": [
                "./**/*"
              ],
              "exclude": [
                "node_modules",
                ".dependencies",
                "dist"
              ]
            },
            "development": false,
            "copyPolicy": {
              "ignorePatterns": [
                "package.json",
                "package-lock.json"
              ],
              "disable": false
            }
          }
        }
      }
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn"
  },
  "overrides": {
    "*": {
      "devDependencies": {
        "@types/node": "+"
      }
    }
  }
}
