  },
  "optionalDependencies": {
    "cypress": "^4.9.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "emulators": "firebase emulators:start --import=dev --project=alt-zap-dev",
    "firestore-export": "firebase emulators:export dev --project=alt-zap-dev",
    "start": "gatsby develop",
    "build": "gatsby build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,js,json}\"",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "bit-export": "bit export lucis.alt",
    "bit-tag-minor": "bit tag --all --minor",
    "bit-tag-patch": "bit tag --all --patch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "bit": {
    "env": {
      "compiler": {
        "bit.envs/compilers/test@0.0.1": {
          "options": {
            "file": "../react-typescript-compiler/components/compilers/react-typescript"
          },
          "compilerArguments": [
            "--declaration",
            "--skipLibCheck"
          ],
          "rawConfig": {
            "tsconfig": {
              "compilerOptions": {
                "skipLibCheck": true,
                "lib": [
                  "dom",
                  "es2015",
                  "es7"
                ]
              }
            }
          }
        }
      }
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn"
  },
  "overrides": {
    "*": {
      "devDependencies": {
        "@types/node": "+"
      }
    }
  }
}
